<serviceGroup>
    <service name="FumWS" targetNamespace="http://thaso.de/fum/ws">
        <description>JNLPGenerator service</description>
        <schema schemaNamespace="http://thaso.de/fum/ws"/>
        <parameter name="ServiceClass" locked="false">de.thaso.fum.ws.FumService</parameter>
        <operation name="ping">
            <messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
        </operation>
        <operation name="findLoginUser">
            <messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
        </operation>
        <operation name="storeLoginUser">
            <messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
        </operation>
    </service>
</serviceGroup>
